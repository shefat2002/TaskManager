@using Tasklyne.Models
@model IEnumerable<Project>

@{
    ViewData["Title"] = "Project Index";
}

<h1>Projects</h1>
<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Description</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var project in Model)
        {
            <tr>
                <td>@project.Name</td>
                <td>@project.Description</td>
            </tr>
        }
    </tbody>
</table>
